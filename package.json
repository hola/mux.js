{
  "name": "@hola.org/mux.js",
  "version": "1.0.0-15",
  "description": "A collection of lightweight utilities for inspecting and manipulating video container formats.",
  "repository": {
    "type": "git",
    "url": "https://github.com/videojs/mux.js.git"
  },
  "scripts": {
    "test": "karma start test/karma.conf.js --single-run",
    "build": "grunt",
    "login": "c=`npm c ls`; test \"${c#*@hola}\" != \"$c\" || npm login --scope @hola.org",
    "prepublish": "npm run build",
    "release": "run-s login release:*",
    "release:tag": "git tag v$npm_package_version",
    "release:publish": "npm publish",
    "release:pushtags": "git push --tags origin"
  },
  "keywords": [
    "video",
    "container",
    "transmux",
    "mux",
    "player",
    "hls",
    "mp4",
    "aac",
    "h264"
  ],
  "author": "Brightcove",
  "license": "Apache-2.0",
  "files": [
    "lib",
    "dist"
  ],
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-contrib-concat": "^0.5.0",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-shell": "^1.3.0",
    "karma": "^0.13.9",
    "karma-chrome-launcher": "^0.2.0",
    "karma-firefox-launcher": "^0.1.6",
    "karma-qunit": "^0.1.5",
    "npm-run-all": "^3.1.0",
    "qunitjs": "^1.18.0"
  }
}
